<h2 class="timeline-title">
   Timeline
</h2>
<% let currentYear = null; site.posts.each((item, index) => {
  console.log(item);
  if(currentYear !== item.date.year()) {
      currentYear = item.date.year(); %>
      <div class="line-item">
<div class="timeline">
  <div class="line-date"></div>
  <div class="line-dot current-dot"></div>
  <div class="line-title current-year"><%- currentYear; %></div>
</div>
<div class="line-h"></div>
</div>
 <% } %>

<div class="line-item">
<div class="timeline">
  <div class="line-date">
    <%- item.date.format('MM.DD'); %>
  </div>
  <div class="line-dot">
    
  </div>
  <div class="line-title">
   <a href="<%- config.root %><%-item.path; %>"> <%- item.title; %> </a>
  </div>
</div>
<% if(index !== site.posts.length -1) {%>
<div class="line-h"></div>
<% } %>
</div>
<%
})
%>

  